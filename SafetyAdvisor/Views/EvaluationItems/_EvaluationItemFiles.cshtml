@model SafetyAdvisor.Models.EvaluationItem
<form method="post" enctype="multipart/form-data" action="/backload/uploadhandler">
    @Html.Hidden("objectContext", @Model.Id)
    <div class="row">
        <div class="col-lg-12">
            <div id="dropzone" class="well" >
                <i class="fa fa-plus-circle"></i><span>&nbsp;&nbsp;Add files...or just drag them over and drop them here</span>
                <input type="file" id="fileupload" name="fileupload" multiple="multiple" />
            </div>
        </div>
    </div>
</form>
