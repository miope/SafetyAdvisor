@using Microsoft.AspNet.Identity;
@using SafetyAdvisor.Helpers;

@{
    ViewBag.Title = "Benutzerkonto verwalten";
}
<div class="container">

    @Html.RenderAlerts()

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">@ViewBag.Title <small>Sie sind angemeldet als<b> @User.Identity.Name</b></small></h1>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">            
            @Html.Partial("_ManageAccountPartial")
        </div>
    </div>

</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
